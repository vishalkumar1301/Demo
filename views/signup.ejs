<%- include('topHtml') %>
<%- include('header') %>

<div class="jumbotron px-3">
    <h1 class="display-4">Sign Up</h1>
    <div class="mx-auto" style="width: 300px;">
        
        <form> 
            <div class="form-group">
                <label for="exampleInputEmail1 my-2">Email address</label>
                <input type="email" class="form-control my-2" id="email" aria-describedby="emailHelp" placeholder="Enter email">
            </div>
            <div class="form-group">
                <label for="exampleInputPassword1 my-2">Password</label>
                <input type="password" class="form-control my-2" id="password" placeholder="Password">
            </div>
            <div class="form-group">
                <label for="exampleInputPassword1 my-2">Confirm Password</label>
                <input type="password" class="form-control my-2" id="confirmPassword" placeholder="Confirm Password">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>

</div>

<%- include('bottomHtml') %>